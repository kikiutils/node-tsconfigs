{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "lib": ["ESNext"],
    "moduleDetection": "force",
    "module": "Preserve",
    "allowImportingTsExtensions": true,
    "allowJs": true,
    "noEmit": true
  }
}
